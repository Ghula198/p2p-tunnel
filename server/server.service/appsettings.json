{
  "Udp": 5410, //服务器监听的 udp端口，需要重启
  "Tcp": 5410, //服务器监听的 tcp端口，需要重启
  "ConnectLimit": 0, //连接限流，一分钟内，同ip可连接数，0不限制，需要重启
  "TcpBufferSize": 3, //0-1KB 1-2KB 2-4KB 3-8KB 4-16KB 5-32KB 6-64KB 7-128KB 8-256KB 9-512KB 10-1024KB
  "RegisterTimeout": 5000, //注册超时，tcp连接后，多久没注册，就断开
  "TimeoutDelay": 20000, //超时强制下线时间 ms
  "EncodePassword": "", //加密密钥32位，为空则客户端必须为空，不为空则客户端无要求
  "AdminGroup": "", //管理员分组，当不启用账号验证时，配置一个分组编号，使此分组中的连接可以使用全部权限，方便配置
  "RegisterEnable": true, //开启登入，禁用后，仅 AdminGroup 超级管理员分组可登入
  "RelayEnable": false, //允许所有人使用中继，包括匿名
  "NoDelay": false //禁用TCP Delay算法
}