{
  "Udp": 5410, //服务器监听的 udp端口，需要重启
  "Tcp": 59410, //服务器监听的 tcp端口，需要重启
  "ConnectLimit": 0, //连接限流，一分钟内，同ip可连接数，0不限制，需要重启
  "TcpBufferSize": 8192, //每个注册连接的buffer size，需要重启
  "RegisterTimeout": 5000, //注册超时，tcp连接后，多久没注册，就断开
  "TimeoutDelay": 20000, //超时强制下线时间 ms
  "EncodePassword": "", //加密密钥32位，为空则客户端必须为空，不为空则客户端无要求
  "VerifyAccount": false, //是否验证账号，开启后，需要验证登入账号，无法使用游客登入
  "RegisterEnable": true, //是否开启注册，开启后所有账号均可登入
  "RelayEnable": false //是否开启服务端中继  开启后，所有账号均可使用中继

}